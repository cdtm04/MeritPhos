<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>

    <color name="themecolor">#135587</color>
    <color name="themecolor_pressed">#0e3c5e</color>
    <color name="themebackgroundcolor">#ffffff</color>
    <color name="usernamecolor">#095287</color>

    <style name="style_count_post_folows_text">
        <item name="android:layout_width">wrap_content</item>
